// Function to print Fibonacci sequence up to 'n'
void printFibonacci(int n) {
    int a;
    int b;
    int next;

    a = 0;
    b = 1;

    output(a);
    output(b);

    next = a + b;
    while (next <= n) {
        output(next);
        a = b;
        b = next;
        next = a + b;
    }
}

void main(void) {
    int num;
    int arr[10];
    
    num = input();

    arr_acc = input();          // will be used for IndexVar and index errors
    arr[arr_acc] = 5;

    // Print Fibonacci sequence
    printFibonacci(num);

    return;
}